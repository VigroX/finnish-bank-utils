  "bugs": {
    "url": "https://github.com/vkomulai/finnish-bank-utils/issues"
  },
  "homepage": "https://github.com/vkomulai/finnish-bank-utils",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-eslint": "^4.0.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.2.0",
    "mocha": "^2.1.0",
